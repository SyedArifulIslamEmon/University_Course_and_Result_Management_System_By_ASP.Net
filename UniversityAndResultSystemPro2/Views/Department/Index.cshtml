@{
    ViewBag.Title = "Save Department";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link href="~/Content/style.css" rel="stylesheet" />

    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <script src="~/Scripts/jquery.min.js"></script>

    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/jquery-1.10.2.js"></script>

  

</head>
<body>

    <form id="saveform" role="form" data-toggle="validator">
        <div style="margin-left: 425px;">
            <h1 style="font-size: 25px;">Save Department</h1>
            <div class="form-group">
                <label for="code">Code:</label>
                <input type="text" class="form-control" placeholder="Code" name="code" id="code" pattern=".{2,7}" required title="2 to 7 characters">
            </div>
            <div class="form-group">
                <label for="Name">Name:</label>
                <input type="text" class="form-control" placeholder="Name" name="Name" id="Name" required>

            </div>
            <button type="submit" class="btn btn-primary" value="save">Save</button>
        </div>

    </form>

   

</body>
</html>
